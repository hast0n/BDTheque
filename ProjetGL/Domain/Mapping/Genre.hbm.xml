    <!--Mapping des propriétés de la classe-->
    <id name="Id" column="genre_id" type="int">
      <generator class="native"></generator>
    </id>
    <property name="Name" column="genre_name" not-null="true"/>

    <!-- Mapping des associations de la classe -->

  </class>
</hibernate-mapping>
